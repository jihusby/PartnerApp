<?xml version='1.0' encoding='utf-8'?>
<widget id="org.itema.godfoten" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Godfoten</name>
    <description>
        RBK partnerapp
    </description>
    <author email="firmapost@itema.no" href="http://www.itema.no">
        Itema AS
    </author>
    <content src="index.html" />
    <preference name="permissions" value="none" />
    <preference name="phonegap-version" value="3.5.0" />
	<preference name="orientation" value="portrait" />
    <preference name="ios-orientation-ipad" value="portrait" />
    <preference name="target-device" value="universal" />
    <preference name="fullscreen" value="true" />
    <preference name="webviewbounce" value="true" />
    <preference name="prerendered-icon" value="true" />
    <preference name="stay-in-webview" value="false" />
    <preference name="detect-data-types" value="true" />
    <preference name="exit-on-suspend" value="false" />
    <preference name="show-splash-screen-spinner" value="true" />
    <preference name="auto-hide-splash-screen" value="true" />
    <preference name="SplashScreen" value="screen"/>
    <preference name="SplashScreenDelay" value="2000" />
    <preference name="disable-cursor" value="false" />
    <preference name="android-minSdkVersion" value="7" />
    <preference name="android-installLocation" value="auto" />

	<icon src="www/icon.png" />
    <icon src="www/res/icon/android/icon-36-ldpi.png" platform="android" height="36" width="36" density="ldpi"/>
    <icon src="www/res/icon/android/icon-48-mdpi.png" platform="android" height="48" width="48" density="mdpi"/>
    <icon src="www/res/icon/android/icon-72-hdpi.png" platform="android" height="72" width="72" density="hdpi"/>
    <icon src="www/res/icon/android/icon-96-xhdpi.png" platform="android" height="96" width="96" density="xhdpi"/>
	<icon src="www/res/icon/ios/icon-36-ldpi.png" platform="ios" width="36" height="36"  />
	<icon src="www/res/icon/ios/icon-48-mdpi.png" platform="ios" width="48" height="48" />
	<icon src="www/res/icon/ios/icon-57.png" platform="ios" width="57" height="57" />
	<icon src="www/res/icon/ios/icon-57-2x.png" platform="ios" width="114" height="114" />
	<icon src="www/res/icon/ios/icon-72.png" platform="ios" width="72" height="72" />
	<icon src="www/res/icon/ios/icon-72-2x.png" platform="ios" width="144" height="144" />
	<icon src="www/res/icon/ios/icon-72-hdpi.png" platform="ios" width="72" height="72" />
	<icon src="www/res/icon/ios/icon-96-xhdpi.png" platform="ios" width="96" height="96" />
	<icon src="www/res/icon/ios/icon-small.png" platform="ios" width="29" height="29" />
	<icon src="www/res/icon/ios/icon-small@2x.png" platform="ios" width="58" height="58" />
	<icon src="www/res/icon/ios/icon-40.png" platform="ios" width="40" height="40" />
	<icon src="www/res/icon/ios/icon-40@2x.png" platform="ios" width="80" height="80" />
	<icon src="www/res/icon/ios/icon-50.png" platform="ios" width="50" height="50" />
	<icon src="www/res/icon/ios/icon-50@2x.png" platform="ios" width="100" height="100" />
	<icon src="www/res/icon/ios/icon-60.png" platform="ios" width="60" height="60" />
	<icon src="www/res/icon/ios/icon-60@2x.png" platform="ios" width="120" height="120" />
	<icon src="www/res/icon/ios/icon-60@3x.png" platform="ios" width="180" height="180" />
	<icon src="www/res/icon/ios/icon-76.png" platform="ios" width="76" height="76" />
	<icon src="www/res/icon/ios/icon-76@2x.png" platform="ios" width="152" height="152" />

    <!-- iPhone -->
    <splash src="www/res/screen/ios/splash-640-960.png" platform="ios" width="640" height="960" />
    <splash src="www/res/screen/ios/splash-640-1136.png" platform="ios" width="640" height="1136" />
    <splash src="www/res/screen/ios/splash-750-1334.png" platform="ios" width="750" height="1334" />
    <splash src="www/res/screen/ios/splash-1242-2208.png" platform="ios" width="1242" height="2208" />
    <!-- iPad -->
    <splash src="www/res/screen/ios/splash-768-1024.png" platform="ios" width="768" height="1024" />
    <splash src="www/res/screen/ios/splash-1536-2048.png" platform="ios" width="1536" height="2048" />

    <!-- Android -->
    <splash src="www/res/screen/android/Splash-200-320.png" platform="android" density="ldpi" />
    <splash src="www/res/screen/android/Splash-320-480.png" platform="android" density="mdpi" />
    <splash src="www/res/screen/android/Splash-480-800.png" platform="android" density="hdpi" />
    <splash src="www/res/screen/android/Splash-720-1280.png" platform="android" density="xhdpi" />
    <splash src="www/res/screen/android/Splash-1080-1440.png" platform="android" density="xxhdpi" />

    <access origin="*" />
	<access origin="tel:*" launch-external="yes" />
	<access origin="mailto:*" launch-external="yes" />
	<access origin="sms:*" launch-external="yes" />
	<access origin="http://*" launch-external="yes" />
	<access origin="https://*" launch-external="yes" />
	<feature name="Notification">
		<param name="android-package" value="org.apache.cordova.dialogs.Notification" />
		<param name="ios-package" value="CDVNotification" />
	</feature>

</widget>
